        <script>
            $('#password').focus(function(){
                $('.password-area').show();
            });

            $('.password-area span').click(function(){
                if( $(this).hasClass('remove-password-area') ) {
                    $('.password-area').hide();
                    return false;
                }
                if( $(this).hasClass('reset') ) {
                    $('#password').val('');
                    return false;
                }
                var pass_length = $('#password').val().length;
                if( pass_length == 6 )
                    return false;

                var val     = $(this).text();
                var old_val = $('#password').val();
                var zz      = old_val + val;
                $('#password').val(zz);
            });
        </script>
